<mvc:View
    displayBlock="true"
    height="100%"
    xmlns="sap.f"
    xmlns:mvc="sap.ui.core.mvc">
    <FlexibleColumnLayout
        id="flexibleColumnLayout"
        backgroundDesign="Solid"
        layout="{/layout}"
    >
        <beginColumnPages>
            <mvc:XMLView id="beginView" viewName="ordermanager.view.List" />
        </beginColumnPages>
        <midColumnPages>
            <mvc:XMLView id="detailView" viewName="ordermanager.view.Detail" />
        </midColumnPages>
    </FlexibleColumnLayout>
</mvc:View>
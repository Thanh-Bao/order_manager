<mvc:View
    controllerName="ordermanager.controller.SalesOrderSet"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core"
>
    <Page id="page" title="{i18n>title}">
        <content />

        <Table items="{TableModel>/EmployeeDetails}">
            <columns>
                <Column>
                    <Text text="dfdf"></Text>
                </Column>
            </columns>
            <items>
                <ColumnListItem>
                    <cells>
                        <Text text="{TableModel>firstName}"></Text>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>

    </Page>
</mvc:View>